		<action in='aA' out='A'/>
	</merge>
	<merge name='consonentMerge'>
		<action in='ttkt' out='kt'/>
	</merge>
</merges>

<buffers>
	<buffer name='prefix'>
		<member id='prefix'/>
	</buffer>
	<buffer name='atoms' action='!!'>
		<member id='whitespace'/>
		<member id='digit'/>
		<member id='atom'/>
		<member id='asis'/>
		<member id='flag' action='x'/>
	</buffer>
	<buffer name='vowel_letters' action='!,|'>
