	</buffer>
	<buffer name='consonents' merge='consonentMerge'>
		<member id='half_form' action='?' map='common_consonent'/>
		<member id='full_form' action='?,|,^' init='vowel_signs' map='common_consonent' required='y'/>
		<member id='half_form_end' map='common_consonent'/>
		<member id='common_consonent'/>  <!-- all consonents go here -->
	</buffer>
	<buffer name='vowel_signs' card='?' init='vowel_sign.a' merge='vowelMerge'>
		<member id='vowel_sign.i' action='!'/>
		<member id='vowel_sign'/>
		<member id='virama' action='-' />
		<errorMessage type='card' message='duplicate vowel signs'/>
		<exists>vowel_letter|full_form</exists>
	</buffer>
	<buffer name='anusvara' card='?'>
		<member id='anusvara'/>
		<errorMessage type='card' message='duplicate vowel signs'/>
		<exists>vowel_letter|full_form</exists>
	</buffer>	
	<buffer name='accent' card='?'>
		<member id='accent'/>
		<errorMessage type='card' message='duplicate accents'/>
		<exists>vowel_letter|full_form|half_form_end</exists>
	</buffer>
	<buffer name='visarga' card='?'>
		<member id='visarga'/>
		<errorMessage type='card' message='duplicate accents'/>
		<exists>vowel_letter|full_form</exists>
	</buffer>
	
</buffers>

